@model IEnumerable<WStore.Models.EmployeeView>
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" charset="utf-8" />
    <title>GetEmployes</title>
</head>
<body>
    <table>
        <thead>
        <th>Id</th>
        <th>Фамилия</th>
        <th>Имя</th>
        <th>Отчество</th>
        <th>Возраст</th>
        </thead>

        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.SurName</td>
            <td>@item.FirstName</td>
            <td>@item.Patronymic</td>
            <td>@item.Age</td>
        </tr>
        }
    </table>
</body>
</html>

