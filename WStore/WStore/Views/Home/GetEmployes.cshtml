@model IEnumerable<WStore.Models.EmployeeView>
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" charset="utf-8" />
    <title>GetEmployes</title>
</head>
<body>
    <table>
        <thead>
        <th>Фамилия</th>
        <th>Имя</th>
        </thead>

        @foreach (var item in Model)
        {
        <tr>
            <td>@item.SurName</td>
            <td>@item.FirstName</td>
            <td>
                @Html.ActionLink("Детали>>", "Details", new { id = item.Id })
            </td>
        </tr>
        }
    </table>
</body>
</html>

