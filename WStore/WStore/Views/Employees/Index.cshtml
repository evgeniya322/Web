@model IEnumerable<WStore.Models.EmployeeView>

@{
    ViewBag.Title="Сотрудники";
}

<h3>@ViewBag.Title</h3>

<table class="table">
    <thead>
    <th>Id</th>
    <th>Фамилия</th>
    <th>Имя</th>
    <th>Отчество</th>
    <th>Возраст</th>
    </thead>

    @foreach (var item in Model)
        {
    <tr>
        <td>@item.Id</td>
        <td>@item.SurName</td>
        <td>@item.FirstName</td>
        <td>@item.Patronymic</td>
        <td>@item.Age</td>
    </tr>
        }
</table>

